{
    "name": "glue-agency/schedule",
    "description": "Schedule plugin for CraftCMS",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "schedule",
        "crontab",
        "job"
    ],
    "homepage": "https://github.com/glue-agency/craft-schedule",
    "support": {
        "issues": "https://github.com/glue-agency/craft-schedule/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "info@glue.be",
            "homepage": "https://glue.be/"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0",
        "nesbot/carbon": "^1.22 || ^2.10",
        "omnilight/yii2-scheduling": "^1.1",
        "symfony/process": "^4.2 || ^5.0 || ^6.0",
        "guzzlehttp/guzzle": "^7.2",
        "panlatent/cron-expression-descriptor": "^1.1"
    },
    "suggest": {
        "ext-intl": "Help translate cron express description"
    },
    "autoload": {
        "psr-4": {
          "GlueAgency\\schedule\\": "src/"
        }
    },
    "extra": {
        "name": "Schedule",
        "handle": "schedule",
        "changelogUrl": "https://raw.githubusercontent.com/glue-agency/craft-schedule/master/CHANGELOG.md",
        "class": "GlueAgency\\schedule\\Plugin"
    },
    "require-dev": {
        "codeception/codeception": "^4.1"
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
